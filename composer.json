{
    "name": "neovg/php-struct",
    "description": "A \"C struct\"-like class with type safe attributes and fluent setter interface.",
    "version": "1.0.2",
    "type": "library",
    "license": "BSD-3-Clause",
    "keywords": [
        "datastructure"
    ],
    "homepage": "https://github.com/NeoVG/php-struct",
    "authors": [
        {
            "name": "Neo Vg",
            "email": "neo@gothnet.eu",
            "role": "Developer",
            "homepage": "https://github.com/NeoVG"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "NeoVg\\Struct\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NeoVg\\Struct\\Test\\": "tests/"
        }
    }
}
